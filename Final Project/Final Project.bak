;; a circle is a (make-circle number string string)
(define-struct circle (radius stroke color)) ;necessary? b/c predefined in racket

;; an ellipse is an (make-ellipse number number string string)
(define-struct ellipse (width height stroke color))

;; a square is a (make-square number string string)
(define-struct square (sidelength stroke color))

;; a rectangle is a (make-rectangle number number string string)
(define-struct rectangle (width height stroke color))

;; a move is a (make-move number string)
(define-struct move (distance direction))

